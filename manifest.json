{
    "name": "Docmark",
    "version": "0.2",
    "description": "Share bookmarks among teammates. It is great for organizing documentation, sharing bunch of links and maintaining them.",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "bookmarks"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://infohub.corp.wayfair.com/*"
            ],
            "js": [
                "ui.js"
            ]
        }
    ],
    "manifest_version": 2
}